{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": false,
        "no-console": false,
        "array-type": [
            true,
            "array"
        ],
        "max-line-length": [
            true,
            100
        ],
        "arrow-parens": false,
        "curly": true,
        "encoding": true,
        "max-classes-per-file": false,
        "no-any": true,
        "no-bitwise": false,
        "no-conditional-assignment": true,
        "no-invalid-this": true,
        "object-literal-sort-keys": false,
        "one-line": false,
        "one-variable-per-declaration": true,
        "prefer-for-of": false,
        "prefer-template": true,
        "quotemark": true,
        "semicolon": true,
        "space-within-parens": false,
        "triple-equals": true,
        "use-isnan": true
    },
    "rulesDirectory": []
}
